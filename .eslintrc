{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended"
      ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "react/no-deprecated": 0,
        "react/jsx-key": 0,
        "react/no-string-refs": 0,
        "react/no-unknown-property": 0
    },
    "parser": "babel-eslint"
  }
